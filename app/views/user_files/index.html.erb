<h1>Listing user_files</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Size</th>
    <th>File</th>
    <th>Thumbnail</th>
    <th>Description</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @user_files.each do |user_file| %>
  <tr>
    <td><%= user_file.name %></td>
    <td><%= user_file.size %></td>
    <td><%= user_file.file_location %></td>
    <td><%= user_file.thumb_location %></td>
    <td><%= user_file.description %></td>
    <td><%= link_to 'Show', user_file %></td>
    <td><%= link_to 'Edit', edit_user_file_path(user_file) %></td>
    <td><%= link_to 'Destroy', user_file, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%# link_to 'New User file', new_user_file_path %>

<div id="jquery-wrapped-fineuploader">
  <div class="qq-uploader"> 
     <div class="qq-upload-button" style="position: relative; overflow; hidden; direction: ltr;" >
        <input multiple="multiple" type="file" name="file" style"position: absolute; right: 0px; top: 0px; font-family: Arial; font-size: 118px; margin: 0px; padding: 0px; cursor: pointer; opacity: 0;">
     </div>
  </div>
</div>
